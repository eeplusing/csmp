package com.moutum.csmp.domain;

import java.util.Date;
import java.util.Set;

/************************************************************************************
 * @Title        : Good.java
 * @Description  : 商品信息
 * @Author       : ZhaoShasha
 * @DateTime     : 2015年5月13日 上午11:30:54
