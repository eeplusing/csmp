public class Good
{

    /**
     * 商品编号
     */
    private int goodId;
    
    /**
     * 商品名称
     */
    private String goodName;
    
    /**
     * 行业属性  1：民用  0：工业
     */
    private int busyType; 
    
    /**
     * 商品规格及介绍
     */
    private String specification;
    
    /**
     * 参数
     */
    private String headValue;
    
    /**
     * 热值
     */
    private int calorificValue;
    
    /**
     * 图片信息
     */
    private Set<Image> images;
    
    /**
     * 商品种类
     */
    private Type type;
    
    /**
     * 商品状态(0:下架；1:上架;2已删除（在页面不显示商品）)
     */
    private int goodStat;
    
    /**
     * 商品上架时间
     */
    private Date upRackTime;
    
    /**
     * 商品下架时间
     */
    private Date downRackTime;
    
    /**
     * 价格
     */
    private Set<Price> prices;
    
    /**
     * 商品产地
     */
    private String goodOrigin;
   
    /**
     * 商品供应商
     */
    private String goodVendor;
    
    /**
     * 是否为热销商品   0：非热销；1：热销；
     */
    private  int goodHot;
    

    public String getGoodOrigin()
    {
        return goodOrigin;
    }

    public void setGoodOrigin(String goodOrigin)
    {
        this.goodOrigin = goodOrigin;
    }

    public String getGoodVendor()
    {
        return goodVendor;
    }

    public void setGoodVendor(String goodVendor)
    {
        this.goodVendor = goodVendor;
    }

    public int getGoodId()
    {
        return goodId;
    }

    public void setGoodId(int goodId)
    {
        this.goodId = goodId;
    }

    public String getGoodName()
    {
        return goodName;
    }

    public void setGoodName(String goodName)
    {
        this.goodName = goodName;
    }

    public int getBusyType()
    {
        return busyType;
    }

    public void setBusyType(int busyType)
    {
        this.busyType = busyType;
    }

    public String getSpecification()
    {
        return specification;
    }

    public void setSpecification(String specification)
    {
        this.specification = specification;
    }

    public String getHeadValue()
    {
        return headValue;
    }

    public void setHeadValue(String headValue)
    {
        this.headValue = headValue;
    }

    public Set<Image> getImages()
    {
        return images;
    }

    public void setImages(Set<Image> images)
    {
        this.images = images;
    }

    public Type getType()
    {
        return type;
    }

    public void setType(Type type)
    {
        this.type = type;
    }

    public int getGoodStat()
    {
        return goodStat;
    }

    public void setGoodStat(int goodStat)
    {
        this.goodStat = goodStat;
    }

    public Date getUpRackTime()
    {
        return upRackTime;
    }

    public void setUpRackTime(Date upRackTime)
    {
        this.upRackTime = upRackTime;
    }

    public Date getDownRackTime()
    {
        return downRackTime;
    }

    public void setDownRackTime(Date downRackTime)
    {
        this.downRackTime = downRackTime;
    }

    public Set<Price> getPrices()
    {
        return prices;
    }

    public void setPrices(Set<Price> prices)
    {
        this.prices = prices;
    }

	public int getCalorificValue() {
		return calorificValue;
	}

	public void setCalorificValue(int calorificValue) {
		this.calorificValue = calorificValue;
	}

    public int getGoodHot()
    {
        return goodHot;
    }

    public void setGoodHot(int goodHot)
    {
        this.goodHot = goodHot;
    }
}