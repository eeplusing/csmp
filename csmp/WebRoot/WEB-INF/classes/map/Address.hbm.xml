<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.moutum.csmp.domain">

	<class name="Address" table="csmp_address_info">
		<id name="addressId" column="address_id">
			<generator class="native" />
		</id>
		<property name="address" column="address" not-null="true"/>
		<property name="postCode" column="post_code" not-null="true"/>
		<property name="consigneName" column="consigne_name" not-null="true"/>
		<property name="consignePhone" column="consigne_phone" not-null="true"/>
	</class>

</hibernate-mapping>