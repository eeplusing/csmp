li {
	float: left;
	width: 160px;
	height: 70px;
	border: 0px solid #F00;
}

.m_li a {
	font-size: 14px;
	color: #000;
	display: block;
	height: 47px;
	padding: 0px 10px;
	font-weight: lighter;
	border: 0px solid #000;
}

.m_li a:hover {
	color: #000;
	text-decoration: none;
}

.fous a {
	font-size: 14px;
	color: #000;
	display: block;
	height: 47px;
	padding: 0px 10px;
	font-weight: lighter;
	border: 0px solid #000;
}

.fous a:hover {
	color: #000;
	text-decoration: none;
}

.m_t {
	background-color: #F9F9F9;
	position: absolute;
	width: 152px;
	border: 4px solid #FFF;
	display: none;
}

.m_t li {
	float: inherit;
	width: 152px;
	height: 20px;
	border-bottom: 2px dashed #EEE;
}

.m_t a {
	width: 130px;
	height: 20px;
	color: #C4D15A;
	display: block;
	padding-top: 3px;
	text-align: center;
	border: 0px solid #F00;
}

.m_t a:hover {
	width: 130px;
	color: #FFF;
	display: block;
	text-align: center;
	background-color: #000;
	border: 0px solid #F00;
}

.m_t span {
	margin: 0px;
}

#main {
	margin: 0px;
	padding: 0px;
	width: 100%;
	min-width: 1450px;
	background-color: #FFF;
	overflow: hidden;
}

#mpage {
	margin: 0px;
	padding: 0px;
	width: 100%;
	height: auto;
	height: 1210px;
	min-height: 1210px;
	border: none;
}